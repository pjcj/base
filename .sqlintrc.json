{
    "rules": {
        "align-column-to-the-first": "error",
        "column-new-line": "error",
        "linebreak-after-clause-keyword": 1,
        "reserved-word-case": [
            1,
            "lower"
        ],
        "space-surrounding-operators": "error",
        "where-clause-new-line": 1,
        "align-where-clause-to-the-first": "error"
    }
}
